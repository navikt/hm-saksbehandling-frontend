{
  "name": "hm-saksbehandling-frontend",
  "version": "0.1.0",
  "type": "module",
  "author": "Team DigiHoT",
  "proxy": "http://localhost:5000",
  "private": true,
  "dependencies": {
    "@types/node": "^12.0.0",
    "compression": "^1.7.4",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.12",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.23.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.1.6"
  },
  "license": "MIT",
  "scripts": {
    "server-dev": "ts-node-dev  --respawn --transpile-only ./server.ts",
    "build-server": "tsc",
    "clean:dist": "rimraf dist/",
    "dev-server": "cross-env-shell NODE_ENV=development ts-node-dev --respawn --transpile-only ./server.ts",
    "dev": "concurrently --kill-others-on-fail \"npm run dev-server\" \"npm start\"",
    "build-exp": "npm run clean:dist &&  parcel build src/index.html --out-dir dist/ --public-url /static"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,json,md}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --rule react-hooks/exhaustive-deps:off"
    ]
  }
}
