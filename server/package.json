{
  "name": "hotsak-frontend-server",
  "version": "0.1.0",
  "type": "module",
  "author": "Team DigiHoT",
  "proxy": "http://localhost:5000",
  "private": true,
  "dependencies": {
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.2",
    "express-session": "^1.17.1",
    "msw": "^0.36.8",
    "node-fetch": "^3.2.3",
    "openid-client": "^3.15.10",
    "tunnel": "0.0.6",
    "web-vitals": "^1.0.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@testing-library/react": "^11.2.6",
    "@types/connect-redis": "^0.0.16",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.12",
    "@types/express-http-proxy": "^1.6.2",
    "@types/express-session": "^1.17.3",
    "@types/node": "^17.0.16",
    "@types/tunnel": "0.0.2",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.23.3",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.6.3"
  },
  "license": "MIT",
  "scripts": {
    "server-dev": "ts-node-dev --respawn --transpile-only ./server.ts",
    "mockend": "cross-env MOCK_BACKEND=true HM_SAKSBEHANDLING_API_URL= ts-node-dev --respawn --transpile-only ./_mock_/mockServer.ts",
    "build-server": "tsc -p .",
    "clean:dist": "rimraf dist/",
    "dev-server": "cross-env-shell NODE_ENV=development SESSION_SECRET=whatever HM_SAKSBEHANDLING_API_URL=http://localhost:8083/api GRUNNDATA_API_URL=http://localhost:8083/api AZURE_APP_WELL_KNOWN_URL=http://host.docker.internal:8080/default/.well-known/openid-configuration ts-node-dev --respawn --transpile-only ./server.ts",
    "dev": "concurrently --kill-others-on-fail \"npm run dev-server\" \"npm start\"",
    "build-exp": "npm run clean:dist && parcel build src/index.html --out-dir dist/ --public-url /static"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,json,md}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --rule react-hooks/exhaustive-deps:off"
    ]
  }
}
